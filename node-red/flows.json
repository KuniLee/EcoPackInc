    {
        "id": "71976d13f1ada451",
        "type": "function",
        "z": "6e52c1301b78909b",
        "name": "function 3",
        "func": "const devices = global.get('devices') || []\nconst skipedDevs = flow.get('skipedDevs')\nconst values = global.get('values')\n\nconst package = []\n\n\ndevices.forEach(( el )=>{\n\n    const id = el.ModbusID\n    const title = el.Name\n    const screen = el.Screen\n    const value = values[id]?.value || null\n    const performance = values[id]?.performance || null\n\n    if (!screen) return\n\n    let status = 'ok'\n\n    if (!el.IsRequested) status = 'disabled'\n    else if (skipedDevs.has(id)) status = 'error'\n\n    package.push({ id, title, screen, status, value, performance})   \n\n});\n\nmsg.payload = package\n\nif (package.length) return msg;\n",
